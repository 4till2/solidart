{
  "extends": "airbnb-base",
  "rules": {
    "camelcase": 0,
    "comma-dangle": 0,
    "dot-notation": 0,
    "func-names": 0,
    "no-console": 0,
    "no-extra-semi": 0,
    "no-global-assign": 0,
    "no-redeclare": 0,
    "no-shadow": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "operator-linebreak": 0,
    "prefer-arrow-callback": 0,
    "prefer-destructuring": 0,
    "prefer-template": 0,
    "quotes": 0,
    "radix": 0,
    "space-before-function-paren": 0,
    "vars-on-top": 0
  }
}
